<!DOCTYPE html>
<html lang="{{lang}}">
<head>
    <meta charset="utf-8">
    <title>{{config.app.fullName}}</title>
    <meta http-equiv="x-ua-compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">

    <script type="text/javascript">
        window.config = {
            env: '{{env}}',
            name: '{{config.app.name}}',
            lang: '{{lang}}',
            fullName: '{{config.app.fullName}}',
            version: '{{config.app.version}}',
            google: {
                apiKey: '{{config.google.browserApiKey}}',
                clientId: '{{config.google.clientId}}',
                scopes: ['https://www.googleapis.com/auth/drive.readonly', 'https://www.googleapis.com/auth/drive']
            },
            backendUrl: '{{config.app.backend}}',
            apiBaseUrl: window.location,
            baseUrl: '{{path}}',
            support: {
                email: '{{config.support.email}}'
            },
            systemMessage: '{{systemMessage}}',
            firebase: {
                isEnabled: {{config.firebase.isEnabled}},
                url: '{{config.firebase.url}}'
            },
            facebook: {
                appId: '{{config.facebook.appId}}',
                apiVersion: '{{config.facebook.apiVersion}}'
            }
        };
    </script>
    <link href="//fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet" type="text/css">
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="//cdnjs.cloudflare.com/ajax/libs/bootswatch/3.3.2/paper/bootstrap.min.css" rel="stylesheet"/>
    <link media="all" rel="stylesheet"
          href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css"/>
    <link media="all" rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.0/css/toastr.css"/>
    <link media="all" rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/nprogress/0.1.2/nprogress.min.css"/>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jointjs/0.9.0/joint.min.css"/>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.2/select2.min.css"/>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.2/select2-bootstrap.min.css"/>
    <link rel="stylesheet"
          href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.0.0/css/bootstrap-datetimepicker.min.css"/>

    <link media="screen" rel="stylesheet" href="{{path}}/style.css"/>
    <!--[if IE]>
    <link media="screen" rel="stylesheet" href="{{path}}/style.ie.css"/>
    <![endif]-->

    <!-- start icon-->
    <link rel="apple-touch-icon" sizes="57x57" href="{{path}}/images/icon/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{path}}/images/icon/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{path}}/images/icon/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{path}}/images/icon/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{path}}/images/icon/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{path}}/images/icon/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{path}}/images/icon/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{path}}/images/icon/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{path}}/images/icon/apple-touch-icon-180x180.png">
    <link rel="icon" type="image/png" href="{{path}}/images/icon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="{{path}}/images/icon/favicon-194x194.png" sizes="194x194">
    <link rel="icon" type="image/png" href="{{path}}/images/icon/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="{{path}}/images/icon/android-chrome-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="{{path}}/images/icon/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="{{path}}/images/icon/manifest.json">
    <link rel="shortcut icon" href="{{path}}/images/icon/favicon.ico">
    <meta name="apple-mobile-web-app-title" content="Let's Meet">
    <meta name="application-name" content="Let's Meet">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="{{path}}/images/icon/mstile-144x144.png">
    <meta name="msapplication-config" content="{{path}}/images/icon/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    <!--end icon-->



    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="//oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
    <script>
        window.___gcfg = {
            parsetags: 'explicit'
        };
    </script>
    <script src="https://apis.google.com/js/platform.js" async defer></script>
</head>
<body role="document">
<div id="app-loading" style="text-align: center;">{{config.app.fullName}} is loading. Please wait...</div>
{{#if isDevelopment}}
    <script data-main="{{path}}/init.js" src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.11/require.js"></script>
{{else}}
    <script data-main="{{path}}/init.js" src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.11/require.min.js"></script>
{{/if}}
</body>
</html>
